{
    "name": "fxp/security-bundle",
    "type": "symfony-bundle",
    "description": "A Extended Role-Based Access Control with sharing rules",
    "keywords": ["RBAC", "E-RBAC", "Permission", "Sharing", "Security"],
    "homepage": "https://github.com/fxpio/fxp-security-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^4.3.0",
        "symfony/security-bundle": "^4.3.0",
        "fxp/security": "^2.0.0"
    },
    "require-dev": {
        "symfony/form": "^4.3.0",
        "symfony/translation": "^4.3.0",
        "doctrine/doctrine-bundle": "^1.9.0",
        "doctrine/orm": "^2.6.0"
    },
    "autoload": {
        "psr-4": { "Fxp\\Bundle\\SecurityBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
